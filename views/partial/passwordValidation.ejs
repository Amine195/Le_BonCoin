    <!-- Complexify script -->
    <script  type="text/javascript" src="/js/complexify/complexify.banlist.js"></script>
    <script  type="text/javascript" src="/js/complexify/complexify.js"></script>

    <!-- Bring Complexify -->
    <script>
        $(document).ready(function() {
            $('#password').complexify({}, function(valid, complex) {
                var progress = $('#progress-bar');
                progress.toggleClass("bg-success", valid);
                progress.toggleClass("bg-danger", !valid);
                progress.css({'width': complex+'%'});
            });
        });
    </script>

    <!-- Pidie Javascript -->
    <script type="text/javascript" src="/js/Password.js"></script>

    <!-- Bring Pidie -->
    <script>
        var pidie = new Pidie();
        pidie.passwordValidation();
        pidie.togglePassword('#password-show');
    </script>