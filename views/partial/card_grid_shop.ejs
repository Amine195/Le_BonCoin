<!-- Card -->
<div class="card booking-card hoverable">

    <!-- Row -->
    <div class="row">

         <!-- Div 1 -->
         <div class="col-xl-4">

            <!-- Card image -->
            <div class="view overlay">
                <img class="card-img-top" src="https://mdbootstrap.com/img/Photos/Horizontal/Food/8-col/img (5).jpg" alt="Card image cap">
                <a href="#!">
                    <div class="mask rgba-white-slight"></div>
                </a>
            </div>

        </div>

        <!-- Div 2 -->
        <div class="col-xl-8">

            <!-- Card content -->
            <div class="card-body pt-3 pb-2">
                
                <!-- Title -->
                <h4 class="card-title font-weight-bold mb-2"><a>La Sirena restaurant</a></h4>

                <!-- Data -->
                <ul class="list-unstyled list-inline rating mb-1">
                    <li class="list-inline-item mr-0"><i class="fa fa-star amber-text"> </i></li>
                    <li class="list-inline-item mr-0"><i class="fa fa-star amber-text"></i></li>
                    <li class="list-inline-item mr-0"><i class="fa fa-star amber-text"></i></li>
                    <li class="list-inline-item mr-0"><i class="fa fa-star amber-text"></i></li>
                    <li class="list-inline-item"><i class="fa fa-star-half-o amber-text"></i></li>
                    <li class="list-inline-item"><p class="text-muted mb-1">4.5 (413)</p></li>
                </ul>

                <!-- Text -->
                <p class="card-text mb-1">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
                
                <!-- Button -->
                <a href="#" class="btn btn-flat deep-purple-text font-weight-bold p-0 mx-0 mb-1">Voir Plus <i class="fa fa-chevron-circle-right" aria-hidden="true"></i></a>
    
            </div>

        </div>

    </div>
      
</div>
<!-- Card -->